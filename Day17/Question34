#include <stdio.h>
int main()
{
    int n,x=0,a;
    printf("Enter a number: ");
    scanf("%d", &n);
    for(int i=2;i<n;i++)
    {
        a=n%i;
        if(a==0)
        {
            x=x+1;
        }        
    }
    if(x==0)
    {
        printf("Prime number");
    }
    else
    {
        printf("Not a prime number");
    }
    return 0;
}
