#include <stdio.h>
int main() 
{
    int n;
    printf("Enter size of array: ");
    scanf("%d", &n);
    int nums[n];
    printf("Enter %d elements: ", n);
    for(int i = 0; i < n; i++)
        scanf("%d", &nums[i]);
    int majorityElement = -1;
    int majorityCount = n / 2;
    for(int i = 0; i < n; i++) 
    {
        int count = 0;
        for(int j = 0; j < n; j++) 
        {
            if(nums[j] == nums[i])
                count++;
        }
        if(count > majorityCount) 
        {
            majorityElement = nums[i];
            break;
        }
    }
    printf("Majority element: %d\n", majorityElement);
    return 0;
}
