#include <stdio.h>
int main()
{
    int n,p=1,a;
    printf("Enter a number: ");
    scanf("%d", &n);
    int x=n;
    while(n>0)
    {
        a=n%10;
        if(a%2!=0)
        {
            p=p*a;
        }
        n=n/10;
    }
    printf("Product of odd digits of %d : %d", x, p);
    return 0;
}
