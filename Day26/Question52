#include <stdio.h>
int main() 
{
    int i, j, k;
    for (i = 1; i <= 5; i++) 
    {
        int stars;
        if (i == 1 || i == 5)
            stars = 1;
        else if (i == 2 || i == 4)
            stars = 3;
        else
            stars = 5;
        for (j = 1; j <= stars; j++) 
        {
            printf("*\n");
        }
        if (i != 5)
            printf("\n");
    }
    return 0;
}
