#include <stdio.h>

int main()
{
    int l,b,area,peri;
    printf("Enter the length of rectangle:");
    scanf("%d", &l);
    printf("Enter the breadth of rectangle:");
    scanf("%d", &b);
    area=l*b;
    peri=2*(l+b);
    printf("Area of rectangle:%d\n", area);
    printf("Perimeter of rectangle:%d\n", peri);
    return 0;
}
