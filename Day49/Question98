#include <stdio.h>
#include <string.h>
#include <ctype.h>
int main() 
{
    char name[100];
    int i, lastSpace = -1;
    printf("Enter full name: ");
    scanf("%[^\n]", name);
    printf("Output: ");
    for(i = 0; name[i] != '\0'; i++) 
    {
        if(name[i] == ' ')
            lastSpace = i;
    }
    i = 0;
    while(i <= lastSpace) 
    {
        if(name[i] != ' ' && (i == 0 || name[i-1] == ' ')) 
        {
            printf("%c ", name[i]);
        }
        i++;
    }
    printf("%s\n", &name[lastSpace + 1]);
    return 0;
}
