#include <stdio.h>
int main() 
{
    int a[10][10], rows, cols, i, j;
    int rowSum[10];
    printf("Enter number of rows: ");
    scanf("%d", &rows);
    printf("Enter number of columns: ");
    scanf("%d", &cols);
    printf("Enter matrix elements:\n");
    for(i = 0; i < rows; i++) 
    {
        for(j = 0; j < cols; j++) 
        {
            scanf("%d", &a[i][j]);
        }
    }
    for(i = 0; i < rows; i++) 
    {
        rowSum[i] = 0;
        for(j = 0; j < cols; j++) 
        {
            rowSum[i] += a[i][j];
        }
    }
    printf("\nRow sums: ");
    for(i = 0; i < rows; i++) 
    {
        printf("%d ", rowSum[i]);
    }
    return 0;
}
