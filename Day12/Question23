#include <stdio.h>
int main()
{
    int days,fine=0;
    printf("Enter the no. of late days: ");
    scanf("%d", &days);
    if(days<=0)
    {
        printf("No fine, Book returned on time");
    }
    else if(days<=5)
    {
        fine=days*2;
        printf("Fine=Rs%d", fine);
    }
    else if(days<=10)
    {
        fine=(5*2)+(days-5)*4;
        printf("Fine=Rs%d", fine);
    }
    else if(days<=30)
    {
        fine=(5*2)+(5*4)+(days-10)*6;
        printf("Fine=Rs%d", fine);
    }
    else
    {
        printf("Membership cancelled");
    }
    return 0;
}
